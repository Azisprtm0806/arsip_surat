<div class="tab-pane fade" id="show-suratKeluar">
  <div class="card shadow mb-4 mt-2">
    <div class="card-body">
      <div class="table-responsive">
        <a
          href="/admin/surat-keluar-print"
          class="
            d-none d-sm-inline-block
            btn btn-sm btn-primary
            shadow-sm
            float-right
          "
          style="margin-left: 5px"
          ><i class="fas fa-download fa-sm text-white-50"></i
        ></a>
        <table
          class="table table-bordered"
          id="dataTableSuratKeluar"
          width="100%"
          cellspacing="0"
        >
          <thead>
            <tr>
              <th>No</th>
              <th>Tanggal</th>
              <th>Nomer Surat</th>
              <th>Tujuan</th>
              <th>Perihal</th>
              <th>Keterangan</th>
            </tr>
          </thead>
          <tbody>
            <% for( let i = 0; i < suratKeluar.length; i++ ) { %>
            <tr>
              <td><%= i+1 %></td>
              <td><%= suratKeluar[i].tanggal %></td>
              <td><%= suratKeluar[i].nomerSurat %></td>
              <td><%= suratKeluar[i].tujuan %></td>
              <td><%= suratKeluar[i].perihal %></td>
              <td><%= suratKeluar[i].keterangan %></td>
            </tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
